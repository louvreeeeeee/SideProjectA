{
  "expo": {
    "name": "PestDigibook",
    "slug": "pest-digibook",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/LOGO.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/LOGO-TRANSPARENT.png",
      "resizeMode": "contain",
      "backgroundColor": "#225d41"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/LOGO.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lougesite.PestDetection123",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/LOGO-TRANSPARENT2.png"
    },
    "extra": {
      "eas": {
        "projectId": "f3ccf0ca-a607-4fe7-b82a-0990ecff6f12"
      }
    },
    "plugins": [
  [
    "@react-native-voice/voice",
    {
      "microphonePermission": "Allow PestDigibook to access the microphone",
      "speechRecognitionPermission": "Allow PestDigibook to securely recognize user speech"
    }
  ],
  "expo-font"
],
    "owner": "louvree"
  }
}
